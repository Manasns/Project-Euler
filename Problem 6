public class SumSquareDifference {

	public static void main(String[] args) {
		int sum = 0;
		for (int i = 1; i <= 100; i++) {
			sum += i^2;
		}
		System.out.println(sum);
		int count = 0;
		for (int j = 1; j <= 100; j++) {
			count += j;
		}
		System.out.println(count^2);
		System.out.println(sum - count^2);
	}

}
