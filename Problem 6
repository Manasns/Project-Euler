//Author: Manas Sarma
//Project Euler-Problem 6
public class SumSquareDifference {

	public static void main(String[] args) {
		int sum = 0;
		int count = 0;
		for (int i = 1; i <= 100; i++) {
			sum += Math.pow(i, 2);//sum of squares
			count += i;//sum of the first 100 natural numbers	
		}
		System.out.println(sum);//sum of squares
		System.out.println(Math.pow(count, 2));//square of sum
		System.out.println(Math.pow(count, 2) - sum);//end result
	}

}
