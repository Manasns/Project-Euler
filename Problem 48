//Author: Manas Sarma
//Project Euler-Problem 48
import java.util.Scanner;
public class SelfPowers {

	public static void main(String[] args) {
		long sum = 0;
		for(long i = 1; i <= 1000; i++) {
			sum += Math.pow(i, i);
		}
		System.out.println(sum);//The sum of the results of i to the i
		//Final answer: the last ten digits of the series are 6854775807
	}
}
